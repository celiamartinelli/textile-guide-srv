{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "fabrics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::fabric.fabric",
      "inversedBy": "products"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "VETEMENT ",
        "BEBE",
        "ENFANT",
        "SOUS-VETEMENT",
        "ACCESSOIRE DE MODE",
        "LINGE DE MAISON",
        "AMEUBLEMENT",
        "TECHNIQUES ET INDUSTRIELS",
        "DECORATION",
        "EXTERIEUR",
        "EPI (Equipement de Protection Individuelle)"
      ]
    },
    "icone_product": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "second_category": {
      "type": "enumeration",
      "enum": [
        "Haut",
        "Bas",
        "Vêtements d'extérieur",
        "Vêtements Intérieur",
        "Vêtements bébé",
        "Vêtements enfants",
        "Vêtements adultes",
        "Vêtements de sport",
        "Vêtements de nuit",
        "Sous-Vêtement",
        "Robes et Combinaison",
        "Accessoires",
        "Sacs",
        "Écharpes et foulards",
        "Chapeaux et bonnets",
        "Gants et mitaines",
        "Literie",
        "Serviettes",
        "Accessoire de jardin"
      ]
    },
    "textile_quantity_required": {
      "type": "string"
    },
    "supplies_quantities": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::supplies-quantity.supplies-quantity",
      "inversedBy": "products"
    },
    "level_sewing": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::level-sewing.level-sewing",
      "inversedBy": "product_id"
    }
  }
}
