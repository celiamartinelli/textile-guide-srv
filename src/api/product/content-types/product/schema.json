{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "fabrics": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::fabric.fabric",
      "inversedBy": "products"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "VETEMENT",
        "VETEMENT BEBE",
        "ACCESSOIRE DE MODE",
        "LINGE DE MAISON",
        "AMEUBLEMENT",
        "TECHNIQUES ET INDUSTRIELS",
        "ENFANT ET BEBE",
        "DECORATION",
        "EXTERIEUR"
      ]
    },
    "icone_product": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "second_category": {
      "type": "enumeration",
      "enum": [
        "Haut",
        "Bas",
        "Vêtements d'extérieur",
        "Vêtements Intérieur",
        "Sous-Vêtement",
        "Accessoires",
        "Robes et combinaisons",
        "Tenues de sport",
        "Vêtements de nuit",
        "Vêtements pour Bébé"
      ]
    },
    "textile_quantity_required": {
      "type": "string"
    }
  }
}
